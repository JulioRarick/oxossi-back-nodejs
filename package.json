    "@types/node": "22.7.4",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.1.2",
    "eslint": "8.57.1",
    "prisma": "5.20.0",
    "supertest": "7.0.0",
    "tsup": "8.3.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.2"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.1",
    "@fastify/jwt": "9.0.1",
